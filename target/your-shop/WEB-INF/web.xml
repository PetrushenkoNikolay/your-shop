<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>AdministrationFilter</filter-name>
  	<display-name>AdministrationFilter</display-name>
  	<description></description>
  	<filter-class>administrationFilter.AdministrationFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>AdministrationFilter</filter-name>
  	<url-pattern>/administration.jsp</url-pattern>
  </filter-mapping>
  <servlet>
  	<servlet-name>UploadImageServlet</servlet-name>
  	<display-name>UploadImageServlet</display-name>
  	<description></description>
  	<servlet-class>uploadImageServlet.UploadImageServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ShowProductsServlet</servlet-name>
  	<display-name>ShowProductsServlet</display-name>
  	<description></description>
  	<servlet-class>showProductsServlet.ShowProductsServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AdministrationServlet</servlet-name>
  	<display-name>AdministrationServlet</display-name>
  	<description></description>
  	<servlet-class>administrationServlet.AdministrationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RemoveSessionServlet</servlet-name>
  	<display-name>RemoveSessionServlet</display-name>
  	<description></description>
  	<servlet-class>removeSessionServlet.RemoveSessionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UploadImageServlet</servlet-name>
  	<url-pattern>/UploadImageServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ShowProductsServlet</servlet-name>
  	<url-pattern>/ShowProductsServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AdministrationServlet</servlet-name>
  	<url-pattern>/AdministrationServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RemoveSessionServlet</servlet-name>
  	<url-pattern>/RemoveSessionServlet</url-pattern>
  </servlet-mapping>
</web-app>
